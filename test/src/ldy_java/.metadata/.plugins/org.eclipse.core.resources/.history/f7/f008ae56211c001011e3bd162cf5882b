package Interface;

enum Gender {
	MALE, FEMALE;
}
enum Level {
	GOLD(korName, discountRate), SILVER,BRONZE;
	
	String korName;
	double discountRate;
	
	private Level(String korName,double discountRate) {
		this.korName=korName;
		this.discountRate=discountRate;
	}
}

public class User {

	public static void main(String[] args) {
		
		int code ;
		String name;
		Gender gender;
		Level level;
		
	}

}
