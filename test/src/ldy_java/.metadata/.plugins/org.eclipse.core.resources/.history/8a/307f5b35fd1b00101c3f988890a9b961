package file;

public class FinallyTest {

	public static void main(String[] args) {

		System.out.println("----Start----");
		
		
		//해야할 일(기능)
		try {
			String s = null;
			System.out.println(s.charAt(0));
		} 
		//문제가 생기면 ... 문제가 생겼다고 알려주고 ... 프로그램을 지속해야 한다.
		catch (Exception e) {
			System.out.println("문제가 발생");
			e.printStackTrace();
		}
		System.out.println("----End----");

	}

}
