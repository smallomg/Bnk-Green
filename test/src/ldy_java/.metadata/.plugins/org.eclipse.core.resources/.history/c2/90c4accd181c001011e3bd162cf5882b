package Interface;

enum Day {
	MONDAY, TUESDAY, WEDNESDAY, THURSDAY, FRIDAY, SATURDAY, SUNDAY
}

public class EnumTest1 {

	public static void main(String[] args) {

		Day today = Day.FRIDAY;

		System.out.println(today);
		switch (today) {
		case MONDAY:
			break;
		case TUESDAY:
			break;
		case WEDNESDAY:
			break;
		case THURSDAY:
			break;
		case FRIDAY:
			break;
		case SATURDAY:
			break;
		case SUNDAY:
			break;
		}
	}

}
