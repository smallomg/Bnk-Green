package Interface;

enum Gender {
	MALE, FEMALE;
}
enum Level {
	GOLD("골드", 0.1), 
	SILVER("실버", 0.05),
	BRONZE("브론즈", 0.02);
	
	String korName;
	double discountRate;
	
	private Level(String korName,double discountRate) {
		this.korName=korName;
		this.discountRate=discountRate;
	}
}

public class User {

	public static void main(String[] args) {
		
		int code ;
		String name;
		Gender gender;
		Level level;
		
	}

}
